@base <http://example.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lambdaber: <https://w3id.org/lambda-ber-schema/> .
@prefix nmdc: <https://w3id.org/nmdc/> .
@prefix ns1: <https://w3id.org/lambda-ber-schema/functional_annotation/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

lambdaber:sample-aldh-cofactor a lambdaber:Sample ;
    dcterms:title "Human aldehyde dehydrogenase 2 with NAD+ and substrate" ;
    lambdaber:biophysical_properties [ a ns1:BiophysicalProperty ;
            ns1:error "0.8"^^xsd:float ;
            ns1:experimental_method "differential_scanning_calorimetry" ;
            ns1:measurement_conditions lambdaber:meas-aldh2-001 ;
            ns1:property_type "melting_temperature" ;
            ns1:unit "K" ;
            ns1:value "328.5"^^xsd:float ] ;
    lambdaber:concentration "5.0"^^xsd:float ;
    lambdaber:concentration_unit "mg_per_ml" ;
    lambdaber:description "ALDH2 enzyme co-crystallized with NAD+ cofactor for structural and functional studies" ;
    lambdaber:functional_sites lambdaber:funcsite-aldh2-active ;
    lambdaber:molecular_composition [ a lambdaber:MolecularComposition ;
            lambdaber:description "Human ALDH2 tetramer with bound NAD+" ;
            lambdaber:sequences "MGSSLSRVPPTAGPLGSPLLLQEPALEAAGEEEELQDSDADGEEEKAEEEEKEEKAEDEEER..." ] ;
    lambdaber:molecular_weight [ a nmdc:QuantityValue ;
            lambdaber:numeric_value "217.5"^^xsd:float ;
            lambdaber:unit "kilodaltons" ] ;
    lambdaber:sample_code "ALDH2-2024-001" ;
    lambdaber:sample_type "protein" .

lambdaber:funcsite-aldh2-active a ns1:FunctionalSite ;
    ns1:chain_id "A" ;
    ns1:confidence_score "1.0"^^xsd:float ;
    ns1:conservation_score "0.99"^^xsd:float ;
    ns1:ec_number "1.2.1.3" ;
    ns1:evidence_type "experimental" ;
    ns1:functional_importance "Essential for aldehyde oxidation to carboxylic acid" ;
    ns1:go_terms "GO:0004029"^^xsd:anyURI,
        "GO:0006081"^^xsd:anyURI ;
    ns1:ligand_interactions [ a ns1:LigandInteraction ;
            ns1:binding_affinity "0.18"^^xsd:float ;
            ns1:binding_affinity_type "km" ;
            ns1:binding_affinity_unit "millimolar" ;
            ns1:binding_site_residues "268",
                "458" ;
            ns1:interaction_distance "3.1"^^xsd:float ;
            ns1:interaction_type "hydrogen_bond" ;
            ns1:is_cofactor false ;
            ns1:is_drug_like false ;
            ns1:ligand_id "CHEBI:15343"^^xsd:anyURI ;
            ns1:ligand_name "Acetaldehyde" ;
            ns1:ligand_smiles "CC=O" ],
        [ a ns1:LigandInteraction ;
            ns1:binding_affinity "0.08"^^xsd:float ;
            ns1:binding_affinity_type "km" ;
            ns1:binding_affinity_unit "millimolar" ;
            ns1:binding_site_residues "268",
                "302",
                "303" ;
            ns1:interaction_distance "2.8"^^xsd:float ;
            ns1:interaction_type "hydrogen_bond" ;
            ns1:is_cofactor true ;
            ns1:is_drug_like false ;
            ns1:ligand_id "CHEBI:57540"^^xsd:anyURI ;
            ns1:ligand_name "NAD+" ;
            ns1:ligand_smiles "C1=CC(=C[N+](=C1)[C@H]2[C@@H]([C@@H]([C@H](O2)COP(=O)(O)OP(=O)(O)OC[C@@H]3[C@H]([C@H]([C@@H](O3)N4C=NC5=C(N=CN=C54)N)O)O)O)O)C(=O)N" ] ;
    ns1:pdb_entry "1O00" ;
    ns1:protein_id "P05091" ;
    ns1:residues "268",
        "302",
        "303",
        "395",
        "458" ;
    ns1:site_name "NAD+-binding catalytic site" ;
    ns1:site_type "catalytic_site" ;
    ns1:source_database "uniprot" .

lambdaber:meas-aldh2-001 a ns1:MeasurementConditions ;
    ns1:buffer_composition [ a lambdaber:BufferComposition ;
            lambdaber:components "100 mM NaCl",
                "50 mM Tris-HCl" ] ;
    ns1:ionic_strength "0.1"^^xsd:float ;
    ns1:ph "7.5"^^xsd:float ;
    ns1:temperature "298.15"^^xsd:float .

