@base <http://example.org/> .
@prefix CHMO: <http://purl.obolibrary.org/obo/CHMO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lambdaber: <https://w3id.org/lambda-ber-schema/> .
@prefix nmdc: <https://w3id.org/nmdc/> .
@prefix ns1: <https://w3id.org/lambda-ber-schema/functional_annotation/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

lambdaber:dataset-p53-interactions a lambdaber:Dataset ;
    dcterms:title "Structural basis of p53 tumor suppressor interactions" ;
    lambdaber:data_files lambdaber:data-file-001 ;
    lambdaber:description "Comprehensive structural and functional study of p53 and its interaction partners combining cryo-EM, crystallography, and knowledge base annotations" ;
    lambdaber:experiment_instrument_associations [ a lambdaber:ExperimentInstrumentAssociation ;
            lambdaber:experiment_id lambdaber:exp-run-001 ;
            lambdaber:instrument_id lambdaber:instrument-titan-001 ;
            lambdaber:role "primary" ] ;
    lambdaber:experiment_runs lambdaber:exp-run-001 ;
    lambdaber:experiment_sample_associations [ a lambdaber:ExperimentSampleAssociation ;
            lambdaber:experiment_id lambdaber:exp-run-001 ;
            lambdaber:preparation_id lambdaber:prep-001 ;
            lambdaber:role "target" ;
            lambdaber:sample_id lambdaber:sample-P53-MDM2-001 ] ;
    lambdaber:instruments lambdaber:instrument-titan-001 ;
    lambdaber:sample_preparations lambdaber:prep-001 ;
    lambdaber:samples lambdaber:sample-P53-MDM2-001 ;
    lambdaber:studies lambdaber:study-p53-interactions ;
    lambdaber:study_experiment_associations [ a lambdaber:StudyExperimentAssociation ;
            lambdaber:experiment_id lambdaber:exp-run-001 ;
            lambdaber:study_id lambdaber:study-p53-interactions ] ;
    lambdaber:study_sample_associations [ a lambdaber:StudySampleAssociation ;
            lambdaber:role "target" ;
            lambdaber:sample_id lambdaber:sample-P53-MDM2-001 ;
            lambdaber:study_id lambdaber:study-p53-interactions ] ;
    lambdaber:study_workflow_associations [ a lambdaber:StudyWorkflowAssociation ;
            lambdaber:study_id lambdaber:study-p53-interactions ;
            lambdaber:workflow_id lambdaber:wf-run-001 ] ;
    lambdaber:workflow_experiment_associations [ a lambdaber:WorkflowExperimentAssociation ;
            lambdaber:experiment_id lambdaber:exp-run-001 ;
            lambdaber:workflow_id lambdaber:wf-run-001 ] ;
    lambdaber:workflow_output_associations [ a lambdaber:WorkflowOutputAssociation ;
            lambdaber:file_id lambdaber:data-file-001 ;
            lambdaber:output_type "map" ;
            lambdaber:workflow_id lambdaber:wf-run-001 ] ;
    lambdaber:workflow_runs lambdaber:wf-run-001 .

lambdaber:func-site-001 a ns1:FunctionalSite ;
    ns1:conservation_score "0.92"^^xsd:float ;
    ns1:evidence_type "experimental" ;
    ns1:protein_id "P04637" ;
    ns1:residues "19",
        "23",
        "26" ;
    ns1:site_name "MDM2 binding interface" ;
    ns1:site_type "protein_binding" ;
    ns1:source_database "pdbe_kb" .

lambdaber:mut-eff-001 a ns1:MutationEffect ;
    ns1:effect_on_function "altered_function" ;
    ns1:effect_on_stability "neutral" ;
    ns1:functional_impact_description "Phosphomimetic - reduces MDM2 binding" ;
    ns1:mutation "S15D" ;
    ns1:mutation_type "missense" ;
    ns1:protein_id "P04637" ;
    ns1:source_database "foldx" .

lambdaber:data-file-001 a lambdaber:DataFile ;
    dcterms:title "Final 3D reconstruction" ;
    lambdaber:data_type "volume" ;
    lambdaber:file_format "mrc" ;
    lambdaber:file_name "p53_mdm2_reconstruction.mrc" ;
    lambdaber:file_path "/data/structures/p53_mdm2.mrc" .

lambdaber:instrument-titan-001 a lambdaber:Instrument ;
    dcterms:title "Titan Krios Cryo-EM" ;
    lambdaber:current_status "operational" ;
    lambdaber:description "Titan Krios cryo-electron microscope" ;
    lambdaber:instrument_code "TITAN-001" ;
    lambdaber:manufacturer "Thermo Fisher Scientific" ;
    lambdaber:model "Titan Krios" .

lambdaber:prep-001 a lambdaber:SamplePreparation ;
    dcterms:title "Cryo-EM grid preparation of p53-MDM2 complex" ;
    lambdaber:preparation_type "cryo_em" ;
    lambdaber:sample_id "lambdaber:sample-P53-MDM2-001" .

lambdaber:sample-P53-MDM2-001 a lambdaber:Sample ;
    dcterms:title "p53-MDM2 complex for structural studies" ;
    lambdaber:concentration [ a nmdc:QuantityValue ;
            lambdaber:numeric_value "8.0"^^xsd:float ;
            lambdaber:unit "milligrams per milliliter" ] ;
    lambdaber:functional_sites lambdaber:func-site-001 ;
    lambdaber:molecular_composition [ a lambdaber:MolecularComposition ;
            lambdaber:description "Human p53 N-terminal domain with MDM2" ] ;
    lambdaber:molecular_weight [ a nmdc:QuantityValue ;
            lambdaber:numeric_value "45.5"^^xsd:float ;
            lambdaber:unit "kilodaltons" ] ;
    lambdaber:mutation_effects lambdaber:mut-eff-001 ;
    lambdaber:sample_code "P53-MDM2-001" ;
    lambdaber:sample_type "complex" .

lambdaber:study-p53-interactions a lambdaber:Study ;
    dcterms:title "Structural basis of p53 tumor suppressor interactions" ;
    lambdaber:description "Comprehensive structural and functional study of p53 and its interaction partners combining cryo-EM, crystallography, and knowledge base annotations" .

lambdaber:wf-run-001 a lambdaber:WorkflowRun ;
    dcterms:title "3D reconstruction of p53-MDM2 complex" ;
    lambdaber:software_name "RELION" ;
    lambdaber:software_version "4.0" ;
    lambdaber:workflow_code "WF-P53-001" ;
    lambdaber:workflow_type "refinement" .

lambdaber:exp-run-001 a lambdaber:ExperimentRun ;
    dcterms:title "Cryo-EM data collection of p53-MDM2" ;
    lambdaber:experiment_code "EXP-P53-001" ;
    lambdaber:quality_metrics [ a lambdaber:QualityMetrics ;
            lambdaber:completeness [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "99.5"^^xsd:float ;
                    lambdaber:unit "percent" ] ;
            lambdaber:resolution [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "3.2"^^xsd:float ;
                    lambdaber:unit "Angstroms" ] ] ;
    lambdaber:technique CHMO:0002413 .

