@base <http://example.org/> .
@prefix CHMO: <http://purl.obolibrary.org/obo/CHMO_> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix lambdaber: <https://w3id.org/lambda-ber-schema/> .
@prefix nmdc: <https://w3id.org/nmdc/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

lambdaber:exp_xray_lysozyme_001 a lambdaber:ExperimentRun ;
    dcterms:title "Lysozyme crystal X-ray diffraction" ;
    lambdaber:data_collection_strategy [ a lambdaber:DataCollectionStrategy ;
            lambdaber:collection_mode "oscillation" ;
            lambdaber:dose_per_frame [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "0.0"^^xsd:float ;
                    lambdaber:unit "electrons per Angstrom squared" ] ;
            lambdaber:frame_rate [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "1.0"^^xsd:float ;
                    lambdaber:unit "frames per second" ] ;
            lambdaber:total_dose [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "0.0"^^xsd:float ;
                    lambdaber:unit "electrons per Angstrom squared" ] ;
            lambdaber:total_frames [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "360.0"^^xsd:float ;
                    lambdaber:unit "frames" ] ] ;
    lambdaber:description "X-ray diffraction data collection from lysozyme crystals at ALS beamline 8.3.1" ;
    lambdaber:experiment_code "EXP-XRD-LYS-2024-001" ;
    lambdaber:experiment_date "2024-04-10T09:30:00" ;
    lambdaber:experimental_conditions [ a lambdaber:ExperimentalConditions ;
            lambdaber:atmosphere "Nitrogen stream" ;
            lambdaber:humidity [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "0.0"^^xsd:float ;
                    lambdaber:unit "percent" ] ;
            lambdaber:pressure [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "101.325"^^xsd:float ;
                    lambdaber:unit "kilopascals" ] ;
            lambdaber:temperature [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "-173.0"^^xsd:float ;
                    lambdaber:unit "degrees Celsius" ] ] ;
    lambdaber:operator_id "als_user_042" ;
    lambdaber:processing_status "completed" ;
    lambdaber:quality_metrics [ a lambdaber:QualityMetrics ;
            lambdaber:completeness [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "99.8"^^xsd:float ;
                    lambdaber:unit "percent" ] ;
            lambdaber:r_factor [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "0.182"^^xsd:float ;
                    lambdaber:unit "R-factor" ] ;
            lambdaber:resolution [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "1.5"^^xsd:float ;
                    lambdaber:unit "Angstroms" ] ;
            lambdaber:signal_to_noise [ a nmdc:QuantityValue ;
                    lambdaber:numeric_value "15.2"^^xsd:float ;
                    lambdaber:unit "signal to noise ratio" ] ] ;
    lambdaber:raw_data_location "/data/als/2024/xray/lysozyme/" ;
    lambdaber:technique CHMO:0000156 .

